{
  "name": "chartx",
  "version": "0.0.1",
  "description": "HTML chart state management and render engine",
  "main": "dist/chartx.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel build:watch test",
    "poststart": "babel-node tools/endMessage.js",
    "build": "webpack --config webpack.config.babel.js",
    "build:watch": "npm run build -- --watch",
    "build_prod": "webpack --config webpack.prod.config.babel.js",
    "build_prod:watch": "npm run build_prod -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryancat/chartx.git"
  },
  "author": "Ryan Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryancat/chartx/issues"
  },
  "homepage": "https://github.com/ryancat/chartx#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "colors": "^1.1.2",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "expect": "^21.0.2",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.1",
    "open": "0.0.5",
    "path": "^0.12.7",
    "webpack": "^3.6.0"
  },
  "keywords": [
    "chart"
  ]
}
